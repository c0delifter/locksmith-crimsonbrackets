<main class="container">
    <br/>
    <h4>Add new password:</h4>
    <div class="row">
        <form method="post" action="newpassword" class="col s12">
            <div class="row">
                <div class="input-field col m6 s12">
                    <input id="password_name" name="password_name" type="text" class="validate">
                    <label for="password_name">Name:</label>
                </div>

                <div class="input-field col m6 s12">
                    <input id="password" name="password" type="text" class="validate">
                    <label for="password">Password:</label>
                </div>
            </div>

        <button id="btnSavePassword" type="submit" class="btn">Create</button>
        </form>
    </div>





</main>
